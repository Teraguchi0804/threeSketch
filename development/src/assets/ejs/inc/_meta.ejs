<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1 ,user-scalable=no">
<title><%= meta.title %></title>
<meta name="description" content="<%= meta.description %>">
<meta name="keywords" content="<%= meta.keyword %>">
<meta name="author" content="">

<!-- fb ogp -->     
<meta property="og:title" content="<%= meta.fb.tit %>"/>
<meta property="og:type" content="<%= meta.fb.type %>"/>
<meta property="og:url" content="<%= meta.fb.url %>"/>
<meta property="og:image" content="<%= meta.fb.img %>"/>
<meta property="og:site_name" content="<%= meta.fb.siteName %>"/>
<meta property="og:description" content="<%= meta.fb.des %>"/>

<!-- twitter ogp -->
<meta name="twitter:card" content="<%= meta.tw.card %>">
<meta name="twitter:site" content="<%= meta.tw.siteName %>">
<meta name="twitter:creator" content="<%= meta.tw.creator %>">
<meta name="twitter:url" content="<%= meta.tw.url %>">
<meta name="twitter:title" content="<%= meta.tw.tit %>">
<meta name="twitter:description" content="<%= meta.tw.des %>">
<meta name="twitter:image" content="<%= meta.tw.img %>">