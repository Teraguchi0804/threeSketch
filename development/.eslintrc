{
  "extends": "eslint:recommended",
  "env": {
      "browser": true,
      "es6": true,
      "jquery": true
  },
  "globals": {
      "TweenMax": true,
      "Power1": true,
      "Power2": true,
      "Power3": true,
      "Power4": true
  },
  "parserOptions": {
     "ecmaVersion": 6,
     "sourceType": "module",
  },
  "rules": { //0: チェックしない, 1: warning 2: error      
//      "no-console": 0, //コンソールを使用している
//      "no-alert": 0, //アラートを使用している
//      "quotes": [0, "single"], //シングルクオートを使用していない
//      "consistent-return": 0, //関数の戻り値に何も指定していない
//      "default-case": 0, //switch文でdefaultが設定されていない
//      "eqeqeq": 0, //「===, !==」ではなく「==,!=」を使用している
//      "camelcase": [0, {"properties": "always"}], //キャメルケースを使用していない
//      "no-unused-vars": 0, //使用していない変数がある
//      "no-undef": 0
  }
}